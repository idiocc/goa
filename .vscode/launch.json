{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "env": {
        "NODE_DEBUG": "@goa/koa",
      },
      "console": "integratedTerminal",
      "args": [
        "test/spec",
        "test/mask",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Z",
      "program": "${workspaceFolder}/node_modules/.bin/alanode",
      "console": "integratedTerminal",
      "args": [
        "z",
        "test/application/context.js",
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Koa",
      "program": "${workspaceFolder}/node_modules/.bin/alanode",
      "env": {
        "DEBUG": "@goa/koa",
      },
      "console": "integratedTerminal",
      "args": [
        "koa",
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster For Module",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "console": "integratedTerminal",
      "args": [
        "modules/http-errors/test.js",
        "-a", "-w", "-t", "9999999"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Depack",
      "program": "${workspaceFolder}/modules/delegates/depack/index_.js",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "program": "${workspaceFolder}/example/index.js",
      "env": {
        "NODE_DEBUG": "@goa/koa",
      },
      "console": "integratedTerminal",
      "args": [
        "example/example.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
