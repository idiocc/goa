{
  "name": "@idio/goa",
  "version": "0.0.0-pre",
  "description": "The Koa Fork Compiler With Google Closure Compiler.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "typal types/index.js types/externs.js",
    "build": "yarn-s d b doc",
    "e": "alanode"
  },
  "files": [
    "build",
    "src",
    "externs.js"
  ],
  "externs": "externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/goa.git"
  },
  "keywords": [
    "goa",
    "idio"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/goa/issues"
  },
  "homepage": "https://github.com/idiocc/goa#readme",
  "devDependencies": {
    "@goa/is-generator-function": "^1.0.1",
    "@goa/vary": "^1.0.0",
    "@idio/debug": "^1.0.0",
    "alamode": "^2.1.2",
    "documentary": "^1.27.1",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  }
}
